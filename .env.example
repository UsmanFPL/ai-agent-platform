# AI Agent Platform Environment Variables
# Copy this file to .env and fill in your actual values

# LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
CUSTOM_LLM_API_KEY=your_custom_llm_jwt_token_here

# Custom LLM Configuration
CUSTOM_LLM_BASE_URL=https://onegpt.fplinternal.in/api

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/ai_agent_platform
REDIS_URL=redis://localhost:6379

# Security
JWT_SECRET_KEY=your_jwt_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here

# FlowiseAI Configuration
FLOWISE_API_KEY=your_flowise_api_key_here
FLOWISE_BASE_URL=http://localhost:3001

# LangFuse Configuration
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key_here
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here
LANGFUSE_HOST=http://localhost:3000

# Environment
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO